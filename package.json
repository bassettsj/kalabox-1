{
  "name": "Kalabox",
  "version": "2.0.0",
  "main": "src/index.html",
  "private": true,
  "window": {
    "title": "Kalabox",
    "toolbar": true,
    "frame": true,
    "width": 700,
    "height": 520,
    "position": "center",
    "resizeable": "false"
  },
  "scripts": {
    "postinstall": "node ./node_modules/grunt-protractor-runner/node_modules/.bin/webdriver-manager update",
    "test": "grunt test"
  },
  "dependencies": {
    "lodash": "~2.4.1"
  },
  "browserify": {
    "transform": [
      "debowerify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "angular": {
      "depends": "jquery:$",
      "exports": "angular"
    },
    "angular-bootstrap": {
      "depends": "angular",
      "exports": "angular"
    }
  },
  "devDependencies": {
    "browserify": "^4.2.3",
    "browserify-shim": "^3.6.0",
    "compression": "~1.0.9",
    "debowerify": "^0.8.1",
    "grunt": "^0.4.5",
    "grunt-browserify": "^2.1.4",
    "grunt-contrib-clean": "~0.6.0",
    "grunt-contrib-compress": "^0.10.0",
    "grunt-contrib-connect": "^0.8.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-less": "~0.11.4",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-jscs": "^0.6.2",
    "grunt-karma": "^0.8.3",
    "grunt-node-webkit-builder": "~0.2.1",
    "grunt-protractor-runner": "^1.1.0",
    "jshint-stylish": "^0.4.0",
    "karma": "^0.12.21",
    "karma-chrome-launcher": "^0.1.4",
    "karma-coverage": "^0.2.6",
    "karma-jasmine": "^0.1.5",
    "karma-ng-html2js-preprocessor": "^0.1.0",
    "karma-phantomjs-launcher": "^0.1.4",
    "matchdep": "~0.3.0",
    "ng-html2js": "^1.1.0"
  }
}
